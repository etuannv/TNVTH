@model IEnumerable<HTTT.HaTangApp.Models.BienBanGiaoCa>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            StartDate
        </th>
        <th>
            StartTime
        </th>
        <th>
            EndDate
        </th>
        <th>
            EndTime
        </th>
        <th>
            SenderId
        </th>
        <th>
            ReciverId
        </th>
        <th>
            Note
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
